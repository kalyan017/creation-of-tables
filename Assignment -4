1.	LIST ALL THE EMPLOYEES WHOSE COMMISSION NULL.
select * from emp
where comm is null;
  
-- 2.	LIST ALL THE EMPLOYEES WHO DON’T HAVE A REPORTING MANAGER.
select* from emp
where mgr is null;

-- 3.	LIST ALL THE EMPLOYEES WHO ARE WORKING AS SALESMAN IN DEPTNO 30.
select * from emp
where job='salesman' and deptno=30;

-- 4.	LIST ALL THE EMPLOYEES WHO ARE WORKING AS SALESMAN IN DEPTNO 30 AND SALARY > 1500.
select*from emp
where job='salesman' and deptno=30 and sal>1500;

-- 5.	LIST ALL THE EMPLOYEES WHOSE NAMES STARTS WITH ‘S’ OR ‘A’.
select * from emp
where ename like 's%' or ename like 'a%';

-- 6.	LIST ALL THE EMPLOYEES EXCEPT THOSE WHO ARE WORKING IN DEPTNO 10, 20.
select * from emp
where not deptno in (10,20);

-- 7.	LIST ALL THE EMPLOYEES WHOSE NAME DOESN’T START WITH ‘S’.
select * from emp
where  ename not like 's%';

-- 8.	LIST ALL THE EMPLOYEES WHO ARE HAVING REPORTING MANAGER IN DEPTNO 10.
  select * from emp
  where mgr is not null and deptno=10;

-- 9.	LIST ALL THE EMPLOYEES WHO DON’T HAVE A REPORTING MANAGER IN DEPTNO 10, 30.
select * from emp
where mgr is null and deptno in(10,30); 

-- 10.	LIST ALL THE EMPLOYEES WHOSE COMMISSION IS NULL AND WORKING AS CLERK.
select * from emp 
where comm is null and job='clerk';

-- 11.	LIST ALL THE EMPLOYEES WHO ARE WORKING AS SALESMAN IN DEPTNO 30 WITH SALARY GREATER THAN 2450.
select * from emp 
where job='salesman' and deptno=30 and sal>2450;

-- 12.	LIST ALL THE EMPLOYEES WHO ARE WORKING AS ANALYST IN DEPTNO 20 AND HAVING SAL GREATER THAN 2500.
select * from emp 
where job='analyst' and deptno=20 and sal>2500;

-- 13.	LIST ALL THE EMPLOYEES WHOSE NAME STARTS WITH ‘M’ OR ‘S’.
select * from emp
where ename like 'm%' or ename like 's%';

-- 14.	LIST ALL THE EMPLOYEES WITH ANNUAL_SALARY EXCEPT WHO ARE WORKING IN DEPTNO 30.
select emp.*,sal*12 as annual_salary from emp
where not deptno=30;

-- 15.	LIST ALL THE EMPLOYEES WHOSE NAME DOESN’T END WITH ‘ES’ OR ‘R’.
select * from emp 
where ename not like '%es' and  ename not like '%r';

-- 16.	LIST ALL THE EMPLOYEES WHO ARE HAVING MGR IN DEPTNO 10 ALONG WITH 10% HIKE IN SALARY.
select emp.*,sal+sal*10/100 as hike from emp 
where mgr is not null and deptno=10;

-- 17.	LIST ALL THE EMPLOYEES WHO ARE JOINED AFTER YEAR 81.
select * from emp
where hiredate >'1981-12-31';

-- 18.	LIST ALL THE EMPLOYEES WHO ARE NOT WORKING AS MANAGER AND CLERK IN DEPTNO 10 AND 20 WITH SAL IN THE RANGE OF 1000 TO 3000.
select * from emp
where not job in ('manager','clerk') and deptno in (10,20) and sal  between 1001 and 2999;

-- 19.	LIST ALL THE EMPLOYEES WHO ARE JOINED IN FEB.
select * from emp
where hiredate between '1981-02-01' and '1981-02-28';

-- 20.	LIST ALL THE EMPLOYEES WHO ARE WORKING AS SALESMAN AND HAVING ‘E’ AS THE LAST BUT ONE CHARACTER IN ENAME BUT SALARY HAVING EXACTLY 4 CHARACTERS.
select * from emp
where job='salesman' and  ename like '%e_'  and sal between 1000 and 9999;
